package com.jite.struts.bean;

import java.util.Date;

public class MessageInfo {
	private int messageId;
	private int messageType;
	private String content;
	private int readFlag;
	private UserInfo fromUser;
	private UserInfo toUser;
	private Date sendDate;
	private Date readDate;
	private JobInfo relatedJob;
	private ActivityInfo relatedActivity;
	
	private MessageInfo replyTo;
	private MessageInfo followReply;
	
	public int getMessageType() {
		return messageType;
	}
	public void setMessageType(int messageType) {
		this.messageType = messageType;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public int getReadFlag() {
		return readFlag;
	}
	public void setReadFlag(int readFlag) {
		this.readFlag = readFlag;
	}
	public UserInfo getFromUser() {
		return fromUser;
	}
	public void setFromUser(UserInfo fromUser) {
		this.fromUser = fromUser;
	}
	public UserInfo getToUser() {
		return toUser;
	}
	public void setToUser(UserInfo toUser) {
		this.toUser = toUser;
	}
	public Date getSendDate() {
		return sendDate;
	}
	public void setSendDate(Date sendDate) {
		this.sendDate = sendDate;
	}
	public JobInfo getRelatedJob() {
		return relatedJob;
	}
	public void setRelatedJob(JobInfo relatedJob) {
		this.relatedJob = relatedJob;
	}
	public Date getReadDate() {
		return readDate;
	}
	public void setReadDate(Date readDate) {
		this.readDate = readDate;
	}
	public ActivityInfo getRelatedActivity() {
		return relatedActivity;
	}
	public void setRelatedActivity(ActivityInfo relatedActivity) {
		this.relatedActivity = relatedActivity;
	}
	public int getMessageId() {
		return messageId;
	}
	public void setMessageId(int messageId) {
		this.messageId = messageId;
	}
	public MessageInfo getReplyTo() {
		return replyTo;
	}
	public void setReplyTo(MessageInfo replyTo) {
		this.replyTo = replyTo;
	}
	public MessageInfo getFollowReply() {
		return followReply;
	}
	public void setFollowReply(MessageInfo followReply) {
		this.followReply = followReply;
	}
	
	
	
}
