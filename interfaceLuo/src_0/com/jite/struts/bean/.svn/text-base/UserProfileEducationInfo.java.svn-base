package com.jite.struts.bean;

import java.util.Date;

public class UserProfileEducationInfo {
	private Long userId;
	private Date fromDate;
	private Date endDate;
	private String degree;
	private String school;
	private String schoolRoot;
	private String major;
	private String majorRoot;
	private String majorSub;
	private String description;
	
	
	public Long getUserId() {
		return userId;
	}
	public void setUserId(Long userId) {
		this.userId = userId;
	}
	public Date getFromDate() {
		return fromDate;
	}
	public void setFromDate(Date fromDate) {
		this.fromDate = fromDate;
	}
	public Date getEndDate() {
		return endDate;
	}
	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}
	public String getDegree() {
		return degree;
	}
	public void setDegree(String degree) {
		this.degree = degree;
	}
	public String getSchool() {
		return school;
	}
	public void setSchool(String school) {
		this.school = school;
	}
	public String getMajor() {
		return major;
	}
	public void setMajor(String major) {
		this.major = major;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getSchoolRoot() {
		return schoolRoot;
	}
	public void setSchoolRoot(String schoolRoot) {
		this.schoolRoot = schoolRoot;
	}
	public String getMajorRoot() {
		return majorRoot;
	}
	public void setMajorRoot(String majorRoot) {
		this.majorRoot = majorRoot;
	}
	public String getMajorSub() {
		return majorSub;
	}
	public void setMajorSub(String majorSub) {
		this.majorSub = majorSub;
	}
	
}
